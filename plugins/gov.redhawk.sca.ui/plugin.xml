<?eclipse version="3.4"?>
<plugin>
   <extension-point id="corbaViewParts" name="%extension-point.name" schema="schema/corbaViewParts.exsd"/>
   <extension-point id="scaContentTypes" name="%extension-point.name.0" schema="schema/scaContentTypes.exsd"/>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <navigatorContent
            activeByDefault="true"
            contentProvider="gov.redhawk.sca.ui.ScaContentProvider"
            icon="icons/obj16/sca_explorer.png"
            id="gov.redhawk.ui.navigatorContent"
            labelProvider="gov.redhawk.sca.ui.ScaLabelProvider"
            name="%navigatorContent.name"
            priority="normal"
            providesSaveables="false"
            sortOnly="false">
         <actionProvider
               class="gov.redhawk.sca.internal.ui.actions.OpenWithActionProvider"
               id="gov.redhawk.ui.commonActionProvider">
            <enablement>
               <or>
               <and>
                  <instanceof
                        value="org.eclipse.emf.ecore.EObject">
                  </instanceof>
                  <test
                        forcePluginActivation="true"
                        property="org.eclipse.emf.ecore.namespace"
                        value="http://www.redhawk.gov/model/sca/2.0.0">
                  </test>
               </and>
               <instanceof
                     value="gov.redhawk.model.sca.provider.ScaWaveformsContainerItemProvider">
               </instanceof>
               <instanceof
                     value="gov.redhawk.model.sca.provider.ScaWaveformFactoriesContainerItemProvider">
               </instanceof>
               <instanceof
                     value="gov.redhawk.model.sca.provider.ScaDeviceManagersContainerItemProvider">
               </instanceof>
            </or>
            </enablement>
         </actionProvider>
         <triggerPoints>
            <instanceof
                  value="gov.redhawk.model.sca.ScaDomainManagerRegistry">
            </instanceof>
         </triggerPoints>
         <possibleChildren>
            <or>
               <and>
                  <instanceof
                        value="org.eclipse.emf.ecore.EObject">
                  </instanceof>
                  <test
                        forcePluginActivation="true"
                        property="org.eclipse.emf.ecore.namespace"
                        value="http://www.redhawk.gov/model/sca/2.0.0">
                  </test>
               </and>
               <instanceof
                     value="gov.redhawk.model.sca.provider.ScaWaveformsContainerItemProvider">
               </instanceof>
               <instanceof
                     value="gov.redhawk.model.sca.provider.ScaWaveformFactoriesContainerItemProvider">
               </instanceof>
               <instanceof
                     value="gov.redhawk.model.sca.provider.ScaWaveformExternalPortsItemProvider">
               </instanceof>
               <instanceof
                     value="gov.redhawk.model.sca.provider.ScaDeviceManagersContainerItemProvider">
               </instanceof>
               <adapt
                     type="org.eclipse.core.resources.IResource">
               </adapt>
            </or>
         </possibleChildren>
         <commonSorter
               class="gov.redhawk.sca.ui.ScaViewerSorter">
         </commonSorter>
         <dropAssistant
               class="gov.redhawk.sca.ui.actions.ResourceDropAdapterAssistant"
               id="gov.redhawk.sca.ui.dropAssistant.resource">
            <possibleDropTargets>
               <or>
                  <instanceof
                        value="gov.redhawk.model.sca.ScaFileStore">
                  </instanceof>
                  <instanceof
                        value="org.eclipse.core.resources.IResource">
                  </instanceof>
               </or></possibleDropTargets>
         </dropAssistant>
      </navigatorContent>
      <commonFilter
            activeByDefault="true"
            description="%commonFilter.description"
            id="gov.redhawk.ui.filter.waveform_factories"
            name="%commonFilter.name">
         <filterExpression>
            <instanceof
                  value="gov.redhawk.model.sca.provider.ScaWaveformFactoriesContainerItemProvider">
            </instanceof>
         </filterExpression>
      </commonFilter>
      <commonFilter
            activeByDefault="false"
            description="%commonFilter.description.0"
            id="gov.redhawk.ui.filter.waveform_components"
            name="%commonFilter.name.0">
         <filterExpression>
            <instanceof
                  value="gov.redhawk.model.sca.provider.ScaWaveformsContainerItemProvider">
            </instanceof>
         </filterExpression>
      </commonFilter>
      <commonFilter
            activeByDefault="false"
            description="%commonFilter.description.1"
            id="gov.redhawk.ui.filter.device_managers"
            name="%commonFilter.name.1">
         <filterExpression>
            <instanceof
                  value="gov.redhawk.model.sca.provider.ScaDeviceManagersContainerItemProvider">
            </instanceof>
         </filterExpression>
      </commonFilter>
      <commonFilter
            activeByDefault="false"
            description="%commonFilter.description.2"
            id="gov.redhawk.ui.filter.domain_file_managers"
            name="%commonFilter.name.2">
         <filterExpression>
            <instanceof
                  value="gov.redhawk.model.sca.ScaFileManager">
            </instanceof>
         </filterExpression>
      </commonFilter>
      <commonFilter
            activeByDefault="false"
            class="gov.redhawk.sca.ui.filters.DeviceManagerChildDevicesFilter"
            description="%commonFilter.description.3"
            id="gov.redhawk.ui.filter.child_devices"
            name="%commonFilter.name.3">
      </commonFilter>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewerContentBinding
            viewerId="gov.redhawk.ui.sca_explorer">
         <includes>
            <contentExtension
                  isRoot="true"
                  pattern="gov.redhawk.ui.navigatorContent">
            </contentExtension>
            <contentExtension
                  pattern="gov.redhawk.ui.filter.*">
            </contentExtension>
            <contentExtension
                  pattern="gov.redhawk.ui.drop.*">
            </contentExtension>
		</includes>
      </viewerContentBinding>
      <viewer
            viewerId="gov.redhawk.ui.sca_explorer">
            <popupMenu
                allowsPlatformContributions="true"
                id="gov.redhawk.ui.sca_explorer#PopupMenu">  
             <insertionPoint name="group.new"/>
             <insertionPoint
                   name="group.open"
                   separator="true"/>
             <insertionPoint name="group.openWith"/>
             <insertionPoint
                   name="group.control"
                   separator="true">
             </insertionPoint>   
             <insertionPoint name="group.edit"
                   separator="true"/>   
             <insertionPoint name="group.reorganize" />         
             <insertionPoint
                   name="group.port"
                   separator="true"/>     
             <insertionPoint
                   name="group.build"
                   separator="true"/> 
             <insertionPoint
                   name="group.generate"
                   separator="true"/> 
             <insertionPoint
                   name="group.search"
                   separator="true"/>              
             <insertionPoint
                   name="additions"
                   separator="true"/>              
             <insertionPoint
                   name="group.properties"
                   separator="true"/>
                     </popupMenu>
            <options>
               <property
                     name="org.eclipse.ui.navigator.hideLinkWithEditorAction"
                     value="true">
               </property>
               <property
                     name="org.eclipse.ui.navigator.hideAvailableExtensionsTab"
                     value="true">
               </property>
            </options>
      </viewer>
      <viewerContentBinding
            viewerId="org.eclipse.ui.navigator.ProjectExplorer">
         <includes>
            <contentExtension
                  pattern="gov.redhawk.ui.navigatorContent">
            </contentExtension>
         </includes>
      </viewerContentBinding>
   </extension>
