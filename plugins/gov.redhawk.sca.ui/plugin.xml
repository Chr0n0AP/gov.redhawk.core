   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any?after=group.control">
         <command
               commandId="gov.redhawk.ui.command.connect"
               icon="icons/clcl16/connect.png"
               style="push">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
         <command
               commandId="gov.redhawk.ui.command.disconnect"
               icon="icons/clcl16/disconnect.png"
               style="push">
            <visibleWhen
                  checkEnabled="true">
            </visibleWhen>
         </command>
         <command
               commandId="gov.redhawk.ui.command.resource.start"
               icon="icons/clcl16/play.gif"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false">
                  <adapt
                        type="CF.ResourceOperations">
                  </adapt>
               </iterate>
            </visibleWhen>
         </command>
         <command
               commandId="gov.redhawk.ui.command.resource.stop"
               icon="icons/clcl16/stop.gif"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false">
                  <adapt
