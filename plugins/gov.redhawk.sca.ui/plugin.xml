            </visibleWhen>
         </command>
         <command
               commandId="org.eclipse.ui.edit.delete"
               icon="icons/clcl16/Release.gif"
               label="%command.label.0"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <or>
                     <adapt
                           type="CF.LifeCycleOperations">
                     </adapt>
                     <instanceof
                           value="gov.redhawk.model.sca.ScaEventChannel">
                     </instanceof>
                  </or>
               </iterate>
            </visibleWhen>
         </command>
         <command
