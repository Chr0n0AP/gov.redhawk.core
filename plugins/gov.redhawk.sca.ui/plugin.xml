               </iterate>
            </visibleWhen>
         </command>
         <command
               commandId="org.eclipse.ui.edit.delete"
               label="%command.label.2"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <instanceof
                        value="gov.redhawk.model.sca.ScaConnection">
                  </instanceof>
               </iterate>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?before=group.control">
         <separator
               name="gov.redhawk.sca.ui.separator.refresh"
               visible="true">
         </separator>
         <command
               commandId="org.eclipse.ui.file.refresh"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <adapt
                        type="gov.redhawk.model.sca.IRefreshable">
                  </adapt>
               </iterate>
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="gov.redhawk.sca.internal.ui.handlers.RemoveDomainHandler"
            commandId="org.eclipse.ui.edit.delete">
         <activeWhen>
            <iterate
                  ifEmpty="false"
                  operator="or">
               <instanceof
                     value="gov.redhawk.model.sca.ScaDomainManager">
               </instanceof>
            </iterate>
         </activeWhen>
      </handler>
      <handler
            class="gov.redhawk.sca.internal.ui.handlers.ReleaseHandler"
            commandId="org.eclipse.ui.edit.delete">
         <activeWhen>
            <iterate
                  ifEmpty="false"
                  operator="and">
               <adapt
                     type="CF.LifeCycleOperations">
               </adapt>
            </iterate>
         </activeWhen>
      </handler>
      <handler
            class="gov.redhawk.sca.internal.ui.handlers.ReleaseEventChannelHandler"
            commandId="org.eclipse.ui.edit.delete">
         <activeWhen>
            <iterate
                  ifEmpty="false"
                  operator="and">
               <instanceof
                     value="gov.redhawk.model.sca.ScaEventChannel">
               </instanceof>
            </iterate>
         </activeWhen>
      </handler>
      <handler
            class="gov.redhawk.sca.internal.ui.handlers.ShutdownNodeHandler"
            commandId="org.eclipse.ui.edit.delete">
         <activeWhen>
            <iterate
                  ifEmpty="false"
                  operator="or">
               <adapt
                     type="CF.DeviceManagerOperations">
               </adapt>
            </iterate>
         </activeWhen>
      </handler>
      <handler
            class="gov.redhawk.sca.internal.ui.handlers.ConnectDomainHandler"
            commandId="gov.redhawk.ui.command.connect">
         <activeWhen>
            <iterate
                  ifEmpty="false"
                  operator="and">
               <test
                     property="gov.redhawk.sca.model.dommgr.connected"
                     value="false">
               </test>
            </iterate>
         </activeWhen>
      </handler>
      <handler
            class="gov.redhawk.sca.internal.ui.handlers.DisconnectDomainHandler"
            commandId="gov.redhawk.ui.command.disconnect">
         <activeWhen>
            <iterate
                  ifEmpty="false"
                  operator="and">
               <test
                     property="gov.redhawk.sca.model.dommgr.connected"
                     value="true">
               </test>
            </iterate>
         </activeWhen>
      </handler>
      <handler
            class="gov.redhawk.sca.internal.ui.handlers.ConnectPortHandler"
            commandId="gov.redhawk.ui.command.connect">
         <activeWhen>
            <or>
            <and>
               <count
                     value="2">
               </count>
               <and>
                  <iterate
                        ifEmpty="false"
                        operator="or">
                     <adapt
                           type="gov.redhawk.model.sca.ScaUsesPort">
                     </adapt>
                  </iterate>
                  <iterate
                        ifEmpty="false"
                        operator="or">
                     <or>
                        <adapt
                              type="gov.redhawk.model.sca.ScaProvidesPort">
                        </adapt>
                        <adapt
                              type="gov.redhawk.model.sca.ScaComponent">
                        </adapt>
                     </or></iterate></and>
            </and>
            <and>
               <count
                     value="1">
               </count>
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <or>
                     <adapt
                           type="gov.redhawk.model.sca.ScaUsesPort">
                     </adapt>
                     <adapt
                           type="gov.redhawk.model.sca.ScaProvidesPort">
                     </adapt>
                     <adapt
                           type="gov.redhawk.model.sca.ScaComponent">
                     </adapt>
                  </or>
               </iterate>
            </and>
            </or>
         </activeWhen>
      </handler>
