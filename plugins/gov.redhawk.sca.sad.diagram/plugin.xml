   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?before=additions">
         <command
               commandId="gov.redhawk.sca.sad.diagram.assignAssemblyController"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <and>
                  <count
                        value="1">
                  </count>
                  <iterate>
                     <or>
                        <instanceof
                              value="mil.jpeojtrs.sca.sad.diagram.edit.parts.ComponentPlacementEditPart">
                        </instanceof>
                        <instanceof
                              value="gov.redhawk.sca.sad.diagram.edit.parts.SadComponentInstantiationEditPart">
                        </instanceof></or>
                  </iterate>
               </and>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?before=additions">
         <menu
               id="gov.redhawk.sca.sad.diagram.startOrderSubMenu"
               label="%menu.label">
            <visibleWhen
                  checkEnabled="false">
               <and>
                  <count
                        value="1">
                  </count>
                  <iterate>
                     <or>
                        <instanceof
                              value="mil.jpeojtrs.sca.sad.diagram.edit.parts.ComponentPlacementEditPart">
                        </instanceof>
                        <instanceof
                              value="gov.redhawk.sca.sad.diagram.edit.parts.SadComponentInstantiationEditPart">
                        </instanceof>
                     </or>
                  </iterate>
               </and>
            </visibleWhen>
            <command
                  commandId="gov.redhawk.sca.sad.diagram.moveEarlierInStartOrder"
                  label="%command.label"
                  style="push">
            </command>
            <command
                  commandId="gov.redhawk.sca.sad.diagram.moveLaterInStartOrder"
                  label="%command.label.0"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
