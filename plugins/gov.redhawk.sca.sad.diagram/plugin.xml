<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.gmf.runtime.emf.type.core.elementTypes">
      <metamodel
            nsURI="http://sca.jpeojtrs.mil/sad">
         <adviceBinding
               class="gov.redhawk.sca.sad.diagram.edit.helpers.SadConnectInterfaceEditHelperAdvice"
               id="gov.redhawk.sca.sad.diagram.edit.helpers.SadConnectInterface"
               inheritance="none"
               typeId="mil.jpeojtrs.sca.sad.diagram.SadConnectInterface_4001">
         </adviceBinding>
         <adviceBinding
               class="gov.redhawk.sca.sad.diagram.edit.helpers.SadComponentInstantiationEditHelperAdvice"
               id="gov.redhawk.sca.sad.diagram.edit.helpers.SadComponentInstantiation"
               inheritance="none"
               typeId="mil.jpeojtrs.sca.sad.diagram.SadComponentInstantiation_3002">
         </adviceBinding>
         <adviceBinding
               class="gov.redhawk.sca.sad.diagram.edit.helpers.SoftwareAssemblyEditHelperAdvice"
               id="gov.redhawk.sca.sad.diagram.edit.helpers.SoftwareAssembly_1000"
               inheritance="none"
               typeId="mil.jpeojtrs.sca.sad.diagram.SoftwareAssembly_1000">
         </adviceBinding>
         <adviceBinding
               class="gov.redhawk.sca.sad.diagram.edit.helpers.SadComponentPlacementEditHelperAdvice"
               id="gov.redhawk.sca.sad.diagram.edit.helpers.SadComponentPlacement"
               inheritance="none"
               typeId="mil.jpeojtrs.sca.sad.diagram.SadComponentPlacement_3001">
         </adviceBinding>
         <adviceBinding
               class="gov.redhawk.sca.sad.diagram.edit.HostCollocationEditHelperAdvice"
               id="gov.redhawk.sca.sad.diagram.edit.helpers.HostCollocation"
               inheritance="none"
               typeId="mil.jpeojtrs.sca.sad.diagram.HostCollocation_3006">
         </adviceBinding>
      </metamodel>
   </extension>
   <extension
         point="org.eclipse.gmf.runtime.emf.type.core.elementTypeBindings">
      <clientContext
            id="mil.jpeojtrs.sca.sad.diagram.TypeContext">
         <enablement>
            <test
                  property="org.eclipse.gmf.runtime.emf.core.editingDomain"
                  value="mil.jpeojtrs.sca.sad.diagram.EditingDomain">
            </test>
         </enablement>
      </clientContext>
      <binding
            context="mil.jpeojtrs.sca.sad.diagram.TypeContext">
         <advice
               pattern="gov.redhawk.sca.sad.diagram.edit.helpers.*">
         </advice>
         <elementType
               ref="mil.jpeojtrs.sca.partitioning.diagram.FindByStub_12002">
         </elementType>
         <elementType
               ref="mil.jpeojtrs.sca.partitioning.diagram.NamingService_13011">
         </elementType>
         <elementType
               ref="mil.jpeojtrs.sca.partitioning.diagram.DomainFinder_13012">
         </elementType>
      </binding>
   </extension>
   

   
    
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="mil.jpeojtrs.sca.sad.diagram.edit.parts.UsesPortStubEditPart"
            class="gov.redhawk.sca.sad.diagram.adapters.UsesPortStubAdapterFactory">
         <adapter
               type="gov.redhawk.model.sca.ScaUsesPort">
         </adapter>
         <adapter
               type="gov.redhawk.model.sca.ScaPort">
         </adapter>
      </factory>
      <factory
            adaptableType="mil.jpeojtrs.sca.sad.diagram.edit.parts.SadConnectInterfaceEditPart"
            class="gov.redhawk.sca.sad.diagram.adapters.SadConnectInterfaceAdapterFactory">
         <adapter
               type="gov.redhawk.model.sca.ScaUsesPort">
         </adapter>
         <adapter
               type="gov.redhawk.model.sca.ScaPort">
         </adapter>
         <adapter
               type="gov.redhawk.model.sca.ScaProvidesPort">
         </adapter>
      </factory>
      <factory
            adaptableType="mil.jpeojtrs.sca.sad.diagram.edit.parts.SadComponentInstantiationEditPart"
            class="gov.redhawk.sca.sad.diagram.adapters.SadComponentInstantiationEditPartAdapterFactory">
         <adapter
               type="gov.redhawk.model.sca.ScaComponent">
         </adapter>
         <adapter
               type="gov.redhawk.model.sca.ScaAbstractComponent">
         </adapter>
         <adapter
               type="gov.redhawk.model.sca.ScaPropertyContainer">
         </adapter>
         <adapter
               type="CF.ResourceOperations">
         </adapter>
         <adapter
               type="CF.LifeCycleOperations">
         </adapter>
         <adapter
               type="gov.redhawk.model.sca.IRefreshable">
         </adapter>
         <adapter
               type="gov.redhawk.model.sca.ScaPortContainer">
         </adapter>
         <adapter
               type="CF.LoggingOperations">
         </adapter>
      </factory>
      <factory
            adaptableType="mil.jpeojtrs.sca.sad.diagram.edit.parts.ProvidesPortStubEditPart"
            class="gov.redhawk.sca.sad.diagram.adapters.ProvidesPortStubAdapterFactory">
         <adapter
               type="gov.redhawk.model.sca.ScaProvidesPort">
         </adapter>
         <adapter
               type="gov.redhawk.model.sca.ScaPort">
         </adapter>
      </factory>
   </extension>
   
   <extension point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs contributorId="mil.jpeojtrs.sca.sad.diagram">
         <propertyTab
             category="domain"
             id="property.tab.ComponentProperties"
             label="%propertyTab.label"/>
      </propertyTabs>
   </extension>
   
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="mil.jpeojtrs.sca.sad.diagram">
         <propertySection
               class="gov.redhawk.diagram.sheet.properties.ComponentPropertiesPropertySection"
               enablesFor="1"
               filter="gov.redhawk.sca.sad.diagram.sheet.properties.SadPropertyTabFilter"
               id="property.section.ComponentProperties"
               tab="property.tab.ComponentProperties">
         </propertySection>
      </propertySections>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?before=additions">
         <command
               commandId="gov.redhawk.sca.sad.diagram.assignAssemblyController"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <and>
                  <count
                        value="1">
                  </count>
                  <iterate>
                     <or>
                        <instanceof
                              value="mil.jpeojtrs.sca.sad.diagram.edit.parts.ComponentPlacementEditPart">
                        </instanceof>
                        <instanceof
                              value="gov.redhawk.sca.sad.diagram.edit.parts.SadComponentInstantiationEditPart">
                        </instanceof></or>
                  </iterate>
               </and>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?before=additions">
         <menu
               id="gov.redhawk.sca.sad.diagram.startOrderSubMenu"
               label="%menu.label">
            <visibleWhen
                  checkEnabled="false">
               <and>
                  <count
                        value="1">
                  </count>
                  <iterate>
                     <or>
                        <instanceof
                              value="mil.jpeojtrs.sca.sad.diagram.edit.parts.ComponentPlacementEditPart">
                        </instanceof>
                        <instanceof
                              value="gov.redhawk.sca.sad.diagram.edit.parts.SadComponentInstantiationEditPart">
                        </instanceof>
                     </or>
                  </iterate>
               </and>
            </visibleWhen>
            <command
                  commandId="gov.redhawk.sca.sad.diagram.moveEarlierInStartOrder"
                  label="%command.label"
                  style="push">
            </command>
            <command
                  commandId="gov.redhawk.sca.sad.diagram.moveLaterInStartOrder"
                  label="%command.label.0"
                  style="push">
            </command>
         </menu>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="org.eclipse.ui.category.edit"
            defaultHandler="gov.redhawk.sca.sad.diagram.part.AssignAssemblyControllerCommand"
            description="%command.description"
            id="gov.redhawk.sca.sad.diagram.assignAssemblyController"
            name="%command.name">
      </command>
      <command
            categoryId="org.eclipse.ui.category.edit"
            defaultHandler="gov.redhawk.sca.sad.diagram.part.MoveComponentEarlierInStartOrder"
            description="%command.description.0"
            id="gov.redhawk.sca.sad.diagram.moveEarlierInStartOrder"
            name="%command.name.0">
      </command>
      <command
            categoryId="org.eclipse.ui.category.edit"
            defaultHandler="gov.redhawk.sca.sad.diagram.part.MoveComponentLaterInStartOrder"
            description="%command.description.1"
            id="gov.redhawk.sca.sad.diagram.moveLaterInStartOrder"
            name="%command.name.1">
      </command>
   </extension>
   <extension
         point="org.eclipse.gmf.runtime.common.ui.services.action.contributionItemProviders">
      <contributionItemProvider
            checkPluginLoaded="false"
            class="org.eclipse.gmf.runtime.diagram.ui.providers.DiagramContributionItemProvider">
         <Priority
               name="Low">
         </Priority>
         <popupContribution class="org.eclipse.gmf.runtime.diagram.ui.providers.DiagramContextMenuProvider">
         	<popupStructuredContributionCriteria objectClass="mil.jpeojtrs.sca.sad.diagram.edit.parts.ComponentPlacementEditPart"/>
         	<popupStructuredContributionCriteria objectClass="mil.jpeojtrs.sca.sad.diagram.edit.parts.ComponentPlacementNameEditPart"/>
         	<popupStructuredContributionCriteria objectClass="mil.jpeojtrs.sca.sad.diagram.edit.parts.HostCollocationNameEditPart"/>
         	<popupStructuredContributionCriteria objectClass="mil.jpeojtrs.sca.sad.diagram.edit.parts.HostCollocationEditPart"/>
			<popupStructuredContributionCriteria objectClass="gov.redhawk.sca.sad.diagram.edit.parts.ProvidesPortStubEditPart"/>
         	<popupStructuredContributionCriteria objectClass="gov.redhawk.sca.sad.diagram.edit.parts.ComponentSupportedInterfaceStubEditPart"/>
         	<popupStructuredContributionCriteria objectClass="gov.redhawk.sca.sad.diagram.edit.parts.UsesPortStubEditPart"/>
         	<popupStructuredContributionCriteria objectClass="mil.jpeojtrs.sca.sad.diagram.edit.parts.SadComponentInstantiationUsageNameEditPart"/>
         	<popupStructuredContributionCriteria objectClass="gov.redhawk.sca.sad.diagram.edit.parts.SadComponentInstantiationEditPart"/>
         	<popupStructuredContributionCriteria objectClass="gov.redhawk.sca.sad.diagram.edit.parts.HostCollocationCompartmentEditPart"/>
         	<popupStructuredContributionCriteria objectClass="gov.redhawk.sca.sad.diagram.edit.parts.SadConnectInterfaceEditPart"/>
         	<popupPredefinedItem
                id="deleteFromDiagramAction"
                remove="true">
         	</popupPredefinedItem>
         </popupContribution>
      </contributionItemProvider>
   </extension>
</plugin>
